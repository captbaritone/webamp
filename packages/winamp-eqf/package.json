{
  "name": "winamp-eqf",
  "version": "1.0.0",
  "description": "Parse and create Winamp .EQF files which describe equalizer settings",
  "type": "module",
  "main": "built/index.js",
  "types": "built/index.d.ts",
  "exports": {
    ".": {
      "import": "./built/index.js",
      "types": "./built/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/captbaritone/webamp.git",
    "directory": "packages/winamp-eqf"
  },
  "bugs": {
    "url": "https://github.com/captbaritone/webamp/issues"
  },
  "homepage": "https://github.com/captbaritone/webamp/tree/master/packages/winamp-eqf",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "built/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "winamp",
    "equalizer",
    "parse",
    "create"
  ],
  "author": "Jordan Eldredge",
  "license": "ISC",
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.24",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "buffer-to-arraybuffer": "0.0.4",
    "typescript": "^5.3.3"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "built"
    ],
    "testEnvironment": "jsdom",
    "extensionsToTreatAsEsm": [".ts"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": ["@swc/jest"]
    }
  }
}
